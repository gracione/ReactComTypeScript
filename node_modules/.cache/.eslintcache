tHorarios(response.data));\n    setHorarioEscolhido('');\n  }, [props.data]);\n\n  let horariosDisponivel: any = [];\n\n  horario.forEach((element: any) => {\n    if (horarioEscolhido == element.inicio) {\n      horariosDisponivel.push(\n        <HorarioDisponivel\n          onClick={() => setHorarioEscolhido(element.inicio)}\n        >\n          {element.inicio}\n        </HorarioDisponivel>\n      )\n\n    } else {\n      horariosDisponivel.push(\n        <HorarioSelecionado\n          onClick={() => setHorarioEscolhido(element.inicio)}\n        >\n          {element.inicio}\n        </HorarioSelecionado>\n      )\n\n    }\n  });\n\n  function agendarHorario(data: any, horario: any) {\n    api\n      .post(\"/horario/inserir\", {\n        data: data,\n        horario: horario,\n        idCliente: 1,\n        idTratamento: idTratamento,\n        idFuncionario: 1\n      })\n    history('/home');\n  }\n  return (\n    <Container >\n      <div>\n        <Center>\n          <DataSelecionada>\n            Data Selecionada {props.data + \" \" + horarioEscolhido}\n          </DataSelecionada>\n        </Center>\n        <HorariosDisponivel>\n          {horariosDisponivel}\n        </HorariosDisponivel>\n\n      </div>\n      <button\n        onClick={() => agendarHorario(props.data, horarioEscolhido)}\n      >\n        Agendar\n      </button>\n    </Container>\n  );\n}\n","/var/www/html/React/ReactComTypeScript/src/components/calendario/styles.ts",[],[],"/var/www/html/React/ReactComTypeScript/src/components/horarios/styles.ts",[],[],"/var/www/html/React/ReactComTypeScript/src/paginas/Funcionarios/inserir.tsx",["294","295","296"],[],"/var/www/html/React/ReactComTypeScript/src/paginas/Tratamentos/inserir.tsx",["297","298","299"],[],"/var/www/html/React/ReactComTypeScript/src/paginas/Tratamentos/alterar.tsx",["300"],[],"import { Container, Conteudo } from \"../../styles/global\";\nimport ExcluirDadosApi from \"../../util/excluir\";\nimport { useNavigate } from 'react-route